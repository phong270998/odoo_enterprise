<odoo>
    <data noupdate="1">
        <record id="uae_employee_payroll_structure" model="hr.payroll.structure">
            <field name="name">United Arab Emirates: Monthly Pay</field>
            <field name="country_id" ref="base.ae"/>
            <field name="type_id" ref="l10n_ae_hr_payroll.uae_employee_payroll_structure_type"/>
            <field name="report_id" ref="l10n_ae_hr_payroll.action_report_payslip_ae"/>
            <field name="unpaid_work_entry_type_ids" eval="[
                (4, ref('hr_work_entry_contract.work_entry_type_unpaid_leave')),
                (4, ref('hr_payroll.hr_work_entry_type_out_of_contract')),
                (4, ref('l10n_ae_hr_payroll.uae_sick_leave_0_entry_type')),
            ]"/>
        </record>
        <record id="l10n_ae_hr_payroll.uae_employee_payroll_structure_type" model="hr.payroll.structure.type">
            <field name="default_struct_id" ref="uae_employee_payroll_structure"/>
        </record>
    </data>
</odoo>
